<div class="collapse navbar-collapse collapseNav">
    <ul id="dashboardList" class="nav navbar-nav">
        {{#each this}}
            <li {{#if @first}}class="active"{{/if}}>
                <a href="#/dashboard/{{Id}}">
                    <h2>{{title}}<span class="sr-only">(current)</span></h2>
                    <p class="subTitle">{{description}}</p>
                </a>
            </li>
        {{/each}}
    </ul>
</div>
<a class="addItemBtn" data-toggle="modal" data-target="#addDashboardModal">+ Add Dashboard</a>